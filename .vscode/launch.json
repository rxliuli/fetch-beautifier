{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension from Subdirectory",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/packages/vscode"
      ],
      "outFiles": [
        "${workspaceFolder}/packages/vscode/dist/*.js"
      ]
    },
  ]
}